<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

        
<hibernate-mapping>

	<class name="com.pragim.hibernatefirstappusingmaven.model.Department" table="dept">
	
		<id name="deptId" column="dept_id"/>
		<property name="deptName" length="30" column="dept_name"/>
		<property name="deptCount" column="dept_count"/>
		<property name="colName" column="col_name" length="30"/>
		
	</class>




</hibernate-mapping>